package com.nt;

import java.util.Scanner;

public class DemoArrays {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the integer values holding the Arrays Objects ");
		int sc = s.nextInt();
		Integer a[] = new Integer[sc];
		System.out.println("Enter the :"+sc+"Integer Objects");
          for(int i=0;i<sc;i++) {
        	  a[i]=new Integer(s.nextInt());
          }//for close
          for(int i=0;i<sc;i++) {
        	  System.out.println("display the Objects:");
        	  System.out.println(a[i].toString()+" ");
          }
          for(Integer k:a) {
        	  System.out.print(k.toString()+" ");
          }
	}// main

}// class
